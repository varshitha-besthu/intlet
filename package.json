{
  "name": "intlet",
  "displayName": "intlet",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView: intletView"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "intlet",
          "title": "This is an coding Agent",
          "icon": "media/arrow-capsule.svg"
        }
      ]
    },
    "views": {
      "intlet": [
        {
          "id" : "intletView",
          "name": "Coding Agent Panel",
          "icon": "media/arrow-capsule.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "intlet.helloWorld",
        "title": "Hello World"
      }
    ]
    

  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.104.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
